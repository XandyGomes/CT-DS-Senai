programa {

  funcao inicio() {
    

    //Declaração de variáveis
    real area, altura, lado

    //Entrada de dados
    escreva("Digite a altura do retângulo em centímetro: ")
    leia(altura)
    escreva("Digite o lado do retângulo em centímetro: ")
    leia(lado)

    //Processamento
    area = altura * lado

    //Saída de dados
    escreva("A área do retângulo é: " , area, " centímetro")

  }
}

programa {
  //EXERCÍCIO - Área retângulo
  funcao inicio() {
    real lado, altura, area

    escreva("Entre com o comprimento do lado: ")
    leia(lado)
    escreva("Entre com o comprimento da altura: ")
    leia(altura)

    area = lado * altura

    escreva("A área do retângulo é: ", area)
    
  }
}

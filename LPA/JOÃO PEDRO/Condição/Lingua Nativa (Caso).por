programa
{
	
	funcao inicio()
	{
      inteiro cd

      escreva ("Digite a sua cidadania 1-Brasileiro, 2-Alemão, 3-Inglês, 4-Italiano, 5-Espanhol e 6-Francês: ")
      leia (cd)

      escolha (cd){

      	caso 1: escreva ("\nPortuguês") pare 

      	caso 2: escreva ("\nAlemão") pare

      	caso 3: escreva ("\nInglês") pare 

      	caso 4: escreva ("\nItaliano") pare

      	caso 5: escreva ("\nEspanhol") pare 

      	caso 6: escreva ("\nFrancês") pare

      	caso contrario: escreva (" A sua língua nativa não pode ser verificada")
      }
	}
}
